<%- include ('templates/header'); -%> 

    <div id="header"></div>
    <div class="parallax" data-parallax="scroll" data-z-index="1" data-image-src="/images/wilshot.jpg">
      <h3 class="para-header"><%= type.type %></h3>
    </div>

    <div class="container" id="images">
      <div class="caption">
        
        <p><%= type.summary %></p>
      </div>
    </div>
    <div class="content">
      
      <div class="gg-container">
        <div class="gg-box" >
          <% if (photos.length > 0) { %>
            <%for (i=0; i < photos.length; i ++){ %>
          <img src="/images/lightbox/<%= photos[i] %>" id="swaper">
          <%}%>
          <% }  %>
          
        
      </div>
      <div class="row">
        <div class="col text-center"><input onclick="swapStylesheet('/css/swap.css')" type="button" value="View in B&W" id="changeCSS" href="#"></input></div>
        <div class="col text-center"><input onclick="swapStylesheet('/css/style.css')" type="button" value="View in Colour" id="changeCSS" href="#"></input></div>
      </div>
    </div>




  </main>
  <hr>
  <%- include('templates/footer'); -%>